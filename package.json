{
  "name": "nuxt-clean",
  "version": "1.0.0",
  "description": "My pioneering Nuxt.js project",
  "author": "AdriÃ  Fontcuberta",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts src/ui --tsconfig .",
    "build": "nuxt-ts build src/ui --tsconfig .",
    "start": "nuxt-ts start src/ui --tsconfig .",
    "generate": "nuxt-ts generate src/ui --tsconfig .",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/axios": "^5.3.6",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@types/jest": "^25.1.4",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "vue-jest": "^4.0.0-0"
  }
